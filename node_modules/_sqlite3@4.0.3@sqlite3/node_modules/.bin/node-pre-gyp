#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_node-pre-gyp@0.10.3@node-pre-gyp/bin/node-pre-gyp" "$@"
  ret=$?
else 
  node  "$basedir/../../../_node-pre-gyp@0.10.3@node-pre-gyp/bin/node-pre-gyp" "$@"
  ret=$?
fi
exit $ret
